package kr.or.connect.jdbcexam.dao;

public class JdbcTest {
	
	public static void main(String[] args) {
		RoleDao roleDao = new RoleDao();
		roleDao.addRole(100, "programmer");
		roleDao.addRole(200, "programmer2");
		roleDao.addRole(300, "programmer3");
		roleDao.addRole(400, "programmer4");
		roleDao.allRole();
	}
}
